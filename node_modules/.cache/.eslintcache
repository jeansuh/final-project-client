rc/store/reducers/index.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/containers/NewStudentContainer.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/containers/StudentContainer.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/containers/AllStudentsContainer.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/containers/HomePageContainer.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/containers/AllCampusesContainer.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/containers/CampusContainer.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/store/reducers/campus.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/store/reducers/campuses.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/store/reducers/students.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/store/reducers/student.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/store/thunks.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/containers/Header.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/views/AllStudentsView.js",["144"],"/*==================================================\nAllStudentsView.js\n\nThe Views component is responsible for rendering web page with data provided by the corresponding Container component.\nIt constructs a React component to display the all students view page.\n================================================== */\nimport { Link } from \"react-router-dom\";\n\nconst AllStudentsView = (props) => {\n  const {students, deleteStudent} = props;\n  // If there is no student, display a message\n  if (!students.length) {\n    return (\n    <div>\n      <p>There are no students.</p>\n      <Link to={`newstudent`}>\n        <button>Add New Student</button>\n      </Link>\n    </div>\n    );\n  }\n  \n  // If there is at least one student, render All Students view \n  return (\n    <div>\n      <h1>All Students</h1>\n\n      {students.map((student) => {\n          let name = student.firstname + \" \" + student.lastname;\n          return (\n            <div key={student.id}>\n              <img src={student.imageUrl} width=\"150\" />\n              <Link to={`/student/${student.id}`}>\n                <h2>{name}</h2>\n              </Link>\n              <Link to={`/editstudent/${student.id}`}>\n                <button style={{marginRight:\"10px\"}}>Edit</button>\n              </Link>\n              <button onClick={() => deleteStudent(student.id)}>Delete</button>\n              <hr/>\n            </div>\n          );\n        }\n      )}\n      <br/>\n      <Link to={`/newstudent`}>\n        <button>Add New Student</button>\n      </Link>\n      <br/><br/>\n    </div>\n  );\n};\n\n\nexport default AllStudentsView;","/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/views/NewStudentView.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/views/HomePageView.js",["145","146"],"/*==================================================\nHomePageView.js\n\nThe Views component is responsible for rendering web page with data provided by the corresponding Container component.\nIt constructs a React component to display the home page.\n================================================== */\n\nimport { Link } from \"react-router-dom\";\n\nconst HomePageView = () => {\n  // Render Home page view\n  return (\n    <div >\n      <h1>Home Page</h1>\n      <br/><br/><br/>\n      <div style={{display:\"flex\",justifyContent:\"center\"}}>\n        <div style={{display:\"flex\",flexDirection:\"column\",marginRight:100}}>\n          <img src=\"https://images.fineartamerica.com/images-medium-large-5/1-borzoi--russian-wolfhound-art-canvas-print-sandra-sij.jpg\" width=\"400\" />\n          <br/>\n          <Link to='/campuses'>\n            <button>View All Campuses</button>\n          </Link>\n        </div>\n        <div style={{display:\"flex\",flexDirection:\"column\"}}>\n          <img src=\"https://www.nationalborzoiclub.com/wp-content/uploads/2021/12/Borzoi-Colors-2.jpg\" height=\"300\" />\n          <br/>\n          <Link to='/students'>\n            <button>View All Students</button>\n          </Link>\n        </div>\n      </div>\n    </div>\n  );    \n}\n\nexport default HomePageView;","/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/views/index.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/store/actions/actionTypes.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/store/actions/actionCreators.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/views/AllCampusesView.js",["147"],"/*==================================================\nAllCampusesView.js\n\nThe Views component is responsible for rendering web page with data provided by the corresponding Container component.\nIt constructs a React component to display all campuses.\n================================================== */\nimport PropTypes from \"prop-types\";\nimport { Link } from \"react-router-dom\";\n\nconst AllCampusesView = (props) => {\n  // If there is no campus, display a message.\n  if (!props.allCampuses.length) {\n    return (\n      <div>\n        <p>There are no campuses.</p>\n        <Link to={`newcampus`}>\n          <button>Add New Campus</button>\n        </Link>\n      </div>\n    );\n  }\n\n  // If there is at least one campus, render All Campuses view \n  return (\n    <div>\n      <h1>All Campuses</h1>\n\n      {props.allCampuses.map((campus) => (\n        <div key={campus.id}>\n          <br/>\n          <img src={campus.imageUrl} width=\"200\"/>\n          <Link to={`/campus/${campus.id}`}>\n            <h2>{campus.name}</h2>\n          </Link>\n          <h4>campus id: {campus.id}</h4>\n          <p>{campus.address}</p>\n          <p>{campus.description}</p>\n          <Link to={`/editcampus/${campus.id}`}>\n            <button style={{marginRight:\"10px\"}}>Edit</button>\n          </Link>\n          <button onClick={() => {\n            props.deleteCampus(campus.id);\n\n          }}>\n            Delete\n            </button>\n          <br/><br/><hr/>\n        </div>\n      ))}\n      <br/>\n      <Link to={`/newcampus`}>\n        <button>Add New Campus</button>\n      </Link>\n      <br/><br/>\n    </div>\n  );\n};\n\n// Validate data type of the props passed to component.\nAllCampusesView.propTypes = {\n  allCampuses: PropTypes.array.isRequired,\n};\n\nexport default AllCampusesView;","/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/views/CampusView.js",["148","149","150"],"/*==================================================\nCampusView.js\n\nThe Views component is responsible for rendering web page with data provided by the corresponding Container component.\nIt constructs a React component to display a single campus and its students (if any).\n================================================== */\nimport { Link } from \"react-router-dom\";\n\n// Take in props data to construct the component\nconst CampusView = (props) => {\n    const {campus, deleteStudent, handleDelete} = props;\n\n    if (!campus.students.length) {\n    let msg = \"There are no students enrolled at \" + campus.name;\n    return(\n        <div>\n        <br/>\n            <img src={campus.imageUrl} width = \"150\"/>\n            <h1>{campus.name}</h1>\n            <p>{campus.address}</p>\n            <p>{campus.description}</p>\n            <Link to={`/editcampus/${campus.id}`}>\n                <button style={{marginRight:\"10px\"}}>Edit Campus</button>\n            </Link>\n            <Link to = {`/campuses`}>\n                <button onClick={() => {\n                    props.deleteCampus(campus.id);\n                }}> Delete Campus </button>\n            </Link>\n            <br/>\n            <p><b>{msg}</b></p>\n            <Link to={`/newstudent`}>\n                <button>Add New Student</button>\n            </Link><br/>\n            <div>\n                <br />\n                <Link to={`/campuses`}>Go back</Link>\n            </div>\n        </div>\n    )\n    }\n    \n    // Render a single Campus view with list of its students\n    return (\n    <div>\n        <br/>\n        <img src={campus.imageUrl} width = \"150\"/>\n        <h1>{campus.name}</h1>\n        <p>{campus.address}</p>\n        <p>{campus.description}</p>\n        <Link to={`/editcampus/${campus.id}`}>\n            <button style={{marginRight:\"10px\"}}>Edit Campus</button>\n        </Link>\n        <Link to = {`/campuses`}>\n            <button onClick={() => {\n                props.deleteCampus(campus.id);\n            }}> Delete Campus </button>\n        </Link>\n\n        {campus.students.map( student => {\n        let name = student.firstname + \" \" + student.lastname;\n        return (\n            <div key={student.id} style={{display:\"flex\", justifyContent:\"center\", alignItems:\"center\"}}>\n            <Link to={`/student/${student.id}`}>\n                <h2>{name}</h2>\n            </Link>          \n            <button style={{ display: \"inline-block\", height: \"23px\", marginLeft:\"10px\" }} onClick={() => handleDelete(student.id)}>Delete</button>   \n            </div>\n        );\n        })}\n        <Link to={`/newstudent`}>\n        <button>Add New Student</button>\n        </Link>\n        <div><br /><Link to={`/campuses`}>Go back</Link></div>\n    </div>\n    );\n};\n\nexport default CampusView;","/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/views/StudentView.js",["151"],"/*==================================================\nStudentView.js\n\nThe Views component is responsible for rendering web page with data provided by the corresponding Container component.\nIt constructs a React component to display the single student view page.\n================================================== */\nimport { Link } from \"react-router-dom\";\n\nconst StudentView = (props) => {\n  const { student } = props;\n\n  // Render a single Student view \n  return (\n    <div>\n      <br/>\n      <img src={student.imageUrl} width=\"150\" />\n      <h1>{student.firstname + \" \" + student.lastname}</h1>\n      {(()=> {\n        if(student?.campus){\n          return(\n            <div>\n              <Link to={`/campus/${student.campus.id}`}>\n              <h2>{student.campus.name}</h2>\n          </Link>\n            </div>\n          )\n        } else {\n          return(\n            <div>\n              <h3>The student is not enrolled at any campus.</h3>\n            </div>\n          )\n        }\n      })()}\n      <p><b>GPA:</b> {student.gpa}</p>\n      <p><b>Email:</b> {student.email}</p>\n    </div>\n  );\n\n};\n\nexport default StudentView;","/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/containers/NewCampusContainer.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/views/NewCampusView.js",[],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/views/EditCampusView.js",["152"],"import Button from '@material-ui/core/Button';\nimport Typography from '@material-ui/core/Typography';\nimport { makeStyles } from '@material-ui/core/styles';\n\n// Create styling for the input form\nconst useStyles = makeStyles( () => ({\n    formContainer:{  \n        width: '500px',\n        backgroundColor: '#f0f0f5',\n        borderRadius: '5px',\n        margin: 'auto',\n    },\n    title: {\n        flexGrow: 1,\n        textAlign: 'left',\n        textDecoration: 'none'\n    }, \n    customizeAppBar:{\n        backgroundColor: '#11153e',\n        shadows: ['none'],\n    },\n    formTitle:{\n        backgroundColor:'#c5c8d6',\n        marginBottom: '15px',\n        textAlign: 'center',\n        borderRadius: '5px 5px 0px 0px',\n        padding: '3px'\n    },\n}));\n\nconst EditCampusView = (props) => {\n    const { campus, handleChange, handleSubmit } = props;\n    const classes = useStyles();\n\n    return (\n        <div>\n            <h1>Edit Campus</h1>\n\n            <div className={classes.root}>\n                <div className={classes.formContainer}>\n                    <div className={classes.formTitle}>\n                        <Typography style={{fontWeight: 'bold', fontFamily: 'Courier, sans-serif', fontSize: '20px', color: '#11153e'}}>\n                            Edit a campus\n                        </Typography>\n                    </div>\n                    <form style={{textAlign: 'center'}} onSubmit={(e) => handleSubmit(e)}>\n                        <label style= {{color:'#11153e', fontWeight: 'bold'}}>Name: </label>\n                        <input type=\"text\" key={campus.name} defaultValue={campus.name} name=\"name\"  />\n                        <br/>\n                        <br/>\n\n                        <label style={{color:'#11153e', fontWeight: 'bold'}}>Address: </label>\n                        <input type=\"text\" key={campus.address} defaultValue={campus.address} name=\"address\"  />\n                        <br/>\n                        <br/>\n\n                        <label style={{color:'#11153e', fontWeight: 'bold'}}>Description: </label>\n                        <input type=\"text\" key={campus.description} defaultValue={campus.description} name=\"description\"  />\n                        <br/>\n                        <br/>\n\n                        <label style={{color:'#11153e', fontWeight: 'bold'}}>Campus Image Url: </label>\n                        <input type=\"text\" key={campus.imageUrl} defaultValue={campus.imageUrl} name=\"imageUrl\"  />\n                        <br/>\n                        <br/>\n\n                        <Button variant=\"contained\" color=\"primary\" type=\"submit\">\n                            Submit\n                        </Button>\n                        <br/>\n                        <br/>\n                    </form>\n                    </div>\n            </div>\n        </div>    \n    )\n}\n\nexport default EditCampusView;","/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/containers/EditCampusContainer.js",["153"],"import Header from './Header';\nimport { Component } from 'react';\nimport { connect } from 'react-redux';\nimport { Redirect } from 'react-router-dom';\n\nimport EditCampusView from '../views/EditCampusView';\n\nimport {\n fetchCampusThunk,\n editCampusThunk,\n} from \"../../store/thunks\";\n\nclass EditCampusContainer extends Component {\n\tconstructor(props) {\n\t\tsuper(props);\n\t\tthis.state = {\n\t\t\tname: \"\",\n\t\t\taddress: \"\",\n\t\t\tdescription: \"\",\n\t\t\timageUrl:\"\",\n\t\t\tredirect: false,\n\t\t\tredirectId: null\n\t\t};\n\t\tthis.handleSubmit = this.handleSubmit.bind(this);\n\t\t// this.handleChange = this.handleChange.bind(this);\n\t}\n\n \tcomponentDidMount() {\n    \t// Get campus ID from URL (API link)\n    \tthis.props.fetchCampus(this.props.match.params.id);\n  \t}\n\n\t// handleChange = (event) => {\n\t// \tthis.setState({\n\t// \t\t[event.target.name] : event.target.value\n\t// \t});\n\t// }\n\n\thandleSubmit = async (event) => {\n\t\tevent.preventDefault();\n\n\t\tlet campus ={\n\t\t\tname: event.target.name.value,\n\t\t\taddress: event.target.address.value,\n\t\t\tdescription: event.target.description.value,\n\t\t\timageUrl: this.props.campus.imageUrl,\n\t\t\tid: this.props.campus.id,\n\t\t}\n\t\t// let campus = {\n\t\t// \tname: this.state.name,\n\t\t// \taddress: this.state.address,\n\t\t// \tdescription: this.state.description,\n\t\t// \tid: this.props.campus.id,\n\t\t// };\n\n\t\tlet updatedCampus = await this.props.editCampus(campus);\n\n\t\tthis.setState({\n\t\t\tname: \"\",\n\t\t\taddress: \"\",\n\t\t\tdescription: \"\",\n\t\t\timageUrl:\"\",\n\t\t\tredirect: true,\n\t\t\tredirectId: campus.id\n\t\t})\n\n\t}\n\n\tcomponentWillMount(){\n\t\tthis.setState({\n\t\t\tname: \"\",\n\t\t\taddress: \"\",\n\t\t\tdescription: \"\",\n\t\t\tredirect: false, redirectId: null});\n\t}\n\n\trender(){\n\t\tif(this.state.redirect){\n\t\t\treturn(\n\t\t\t\t<Redirect to = {`/campus/${this.state.redirectId}`}/>\n\t\t\t)\n\t\t}\n\n\t\treturn(\n\t\t\t<div>\n\t\t\t\t<Header/ >\n\t\t\t\t<EditCampusView\n\t\t\t\t\tcampus = {this.props.campus}\n\t\t\t\t\thandleSubmit = {this.handleSubmit}\n\t\t\t\t/>\n\t\t\t</div>\n\t\t)\n\t}\n}\n\nconst mapState = (state) => {\n  return {\n    campus: state.campus,  // Get the State object from Reducer \"campus\"\n  };\n};\n// 2. The \"mapDispatch\" argument is used to dispatch Action (Redux Thunk) to Redux Store.\n// The \"mapDispatch\" calls the specific Thunk to dispatch its action. The \"dispatch\" is a function of Redux Store.\nconst mapDispatch = (dispatch) => {\n  return {\n    fetchCampus: (id) => dispatch(fetchCampusThunk(id)),\n    editCampus: (campus) => dispatch(editCampusThunk(campus)),\n  };\n};\n\nexport default connect(mapState, mapDispatch)(EditCampusContainer);","/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/containers/EditStudentContainer.js",["154"],"/Users/jeansuh/Documents/CS-Webdev/final-project/final-project-client/src/components/views/EditStudentView.js",[],{"ruleId":"155","replacedBy":"156"},{"ruleId":"157","replacedBy":"158"},{"ruleId":"159","severity":1,"message":"160","line":32,"column":15,"nodeType":"161","endLine":32,"endColumn":57},{"ruleId":"159","severity":1,"message":"160","line":18,"column